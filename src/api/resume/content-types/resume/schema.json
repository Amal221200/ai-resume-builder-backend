{
  "kind": "collectionType",
  "collectionName": "resumes",
  "info": {
    "singularName": "resume",
    "pluralName": "resumes",
    "displayName": "Resume",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "resumeId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "user_email": {
      "type": "email",
      "required": true
    },
    "username": {
      "type": "string",
      "required": true
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "jobTitle": {
      "type": "string"
    },
    "themeColor": {
      "type": "string",
      "default": "#222222"
    },
    "summary": {
      "type": "text"
    },
    "experiences": {
      "type": "component",
      "repeatable": true,
      "component": "experience.experience"
    },
    "educations": {
      "type": "component",
      "repeatable": true,
      "component": "education.education"
    },
    "skills": {
      "displayName": "Skills",
      "type": "component",
      "repeatable": true,
      "component": "skill.skills"
    },
    "certificates": {
      "type": "component",
      "repeatable": true,
      "component": "certificate.certificate"
    },
    "projects": {
      "type": "component",
      "repeatable": true,
      "component": "project.project"
    }
  }
}
